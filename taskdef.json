{
      "family": "kanai-task",
      "taskRoleArn": "arn:aws:iam::167551637330:role/ecsTaskExecutionRole",
      "executionRoleArn": "arn:aws:iam::167551637330:role/ecsTaskExecutionRole",
      "networkMode": "awsvpc",
      "containerDefinitions": [
          {
              "name": "httpd",
              "image": "<httpd>",
              "portMappings": [
                  {
                      "containerPort": 80,
                      "hostPort": 80,
                      "protocol": "tcp"
                  }
              ],
              "essential": true,
          }
      ],
      "requiresCompatibilities": [
          "FARGATE"
      ],
      "cpu": "256",
      "memory": "512"
}
